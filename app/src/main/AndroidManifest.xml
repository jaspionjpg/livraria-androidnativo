    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".EsqueciASenha" />
        <activity android:name=".Home" />
        <activity android:name=".AdministracaoLivros" />
        <activity android:name=".NovoLivro" />
        <activity android:name=".EditarLivro" />
        <activity android:name=".AlterarSenha" />
        <activity android:name=".EquipeDesenvolvimento" />
        <activity android:name=".Catalogo" />
        <activity android:name=".TodosLivros" />
        <activity android:name=".LivrosPorCategoria" />
        <activity android:name=".LivrosPorPesquisa" />
        <activity android:name=".DetalheLivro"></activity>
    </application>

</manifest>